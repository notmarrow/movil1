<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameMiHistorial"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylecyan_800">
        <LinearLayout android:id="@+id/linearColumniconocasaone"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowiconocasaone"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameMenu"
                        android:layout_height="@dimen/_74pxv"
                        android:layout_marginBottom="@dimen/_3pxv" android:layout_width="@dimen/_38pxh">
                        <FrameLayout
                            android:id="@+id/frameStackiconocasaone"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_74pxv"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:layout_width="@dimen/_37pxh" style="@style/groupStylewhite_A700cornerRadius">
                            <ImageView
                                android:id="@+id/imageIconoCasaOne"
                                android:layout_gravity="top|center"
                                android:layout_height="@dimen/_25pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginEnd="@dimen/_6pxh"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="@dimen/_25pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconocasa1"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconocasa1"/>
                        </FrameLayout>
                        <FrameLayout
                            android:id="@+id/frameStackiconorelojtwo"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_28pxv"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_35pxh" style="@style/groupStylecyan_800cornerRadius">
                            <ImageView
                                android:id="@+id/imageIconoRelojTwo"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_26pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_5pxh"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_25pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconoreloj2"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconoreloj2"/>
                        </FrameLayout>
                    </FrameLayout>

                    <TextView
                        android:id="@+id/txtTitulo"
                        style="@style/txtLilitaOne20"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_93pxh"
                        android:layout_marginTop="@dimen/_54pxv"
                        android:layout_weight="0.73"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="Almacen"
                        tools:text="Almacen" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearBusqueda"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_2">

                <TextView
                    android:id="@+id/txtBusqueda"
                    style="@style/txtDMSansregular10"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_weight="0.63"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_search"
                    android:textSize="12sp"
                    tools:text="@string/lbl_search" />
                <ImageView android:id="@+id/imageIconoBusqueda"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_228pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconobusqueda"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconobusqueda"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearStatus"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">

                <LinearLayout android:id="@+id/linearBotonOrdenar"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.66"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylewhite_2"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnprice"
                style="@style/groupStylebluegray_100cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="534dp"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/buttonList"
                    android:layout_width="match_parent"
                    android:layout_height="516dp" />
            </LinearLayout>

            <Button
                android:id="@+id/buttonAddItem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Add Item" />
        </LinearLayout>
    </FrameLayout>
</layout>
